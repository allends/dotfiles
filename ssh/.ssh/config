Host github.com-eulerity
	HostName github.com
	User git
	IdentityFile ~/.ssh/eulerity
	IdentitiesOnly yes

Host github.com
	HostName github.com
	User git
	IdentityFile ~/.ssh/personal
	IdentitiesOnly yes

# macOS 1Password SSH agent
Match host * exec "test $(uname) = Darwin"
	IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"
