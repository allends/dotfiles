# This is Git's per-user configuration file.
[user]
	name = Allen Davis-Swing
	email = allen.davisswing@gmail.com
	signingkey = ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAIDw4paomkPUuZfuJsZdWERz3sFBNNm2b7TqU1SUDAks/
[gpg]
	format = ssh
[gpg "ssh"]
	program = /Applications/1Password.app/Contents/MacOS/op-ssh-sign
[commit]
	gpgsign = true
[push]
	autoSetupRemote = true
[core]
	editor = hx

[alias]
	st = status
	co = checkout
	ci = commit
	br = branch
	unstage = reset HEAD --
	st = status --short --branch
